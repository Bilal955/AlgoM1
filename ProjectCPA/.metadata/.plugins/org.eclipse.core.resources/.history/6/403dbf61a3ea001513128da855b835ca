package algorithms;

import java.awt.Point;

public class Arete {
	
	private Point a;
	private Point b;

	public Arete(Point a, Point b) {
		this.a = a;
		this.b = b;
	}

	public Point getPointA() {
		return a;
	}
	
	public Point getPointB() {
		return b;
	}
	
	public double getPoids() {
		return a.distance(b);
	}
	
	public String toString() {
		return "["+a+", "+b+"]";
	}

}
